{
  "name": "node-verifier-schema",
  "version": "1.1.1",
  "description": "verify JSON plain object schema. api input verifier",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test_jshint": "node_modules/.bin/grunt jshint",
    "test_unit": "node_modules/.bin/nodeunit tests/",
    "test_unit_cov": "NODEVERIFIERSCHEMA_COV=1 node_modules/.bin/nodeunit --reporter=lcov tests/",
    "jscov": "node_modules/.bin/jscoverage lib",
    "test": "npm run test_jshint && npm run test_unit",
    "coveralls": "npm run jscov && npm run test_unit_cov | node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aliaksandr-pasynkau/node-verifier-schema.git"
  },
  "keywords": [
    "object",
    "api",
    "validator",
    "verifier",
    "check",
    "checker",
    "json",
    "schema",
    "verify",
    "validate"
  ],
  "author": "aliaksandr-pasynkau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliaksandr-pasynkau/node-verifier-schema/issues"
  },
  "homepage": "https://github.com/aliaksandr-pasynkau/node-verifier-schema",
  "dependencies": {
    "lodash": "^2.4.1",
    "async-iterate": "^0.1.3",
    "js-yaml": "^3.2.3",
    "node-verifier": "~0.5.5"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "colors": "^1.0.3",
    "nodeunit": "^0.9.0",
    "grunt": "^0.4.5",
    "grunto": "^1.2.1",
    "grunt-contrib-jshint": "^0.10.0",
    "jscoverage": "^0.5.9",
    "grunt-cli": "^0.1.13",
    "coveralls": "^2.11.2"
  }
}
